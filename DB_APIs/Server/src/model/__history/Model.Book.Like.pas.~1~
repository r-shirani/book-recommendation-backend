Unit Model.Book.Like;

Interface

Uses
    MVCFramework.Serializer.Commons,
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables;
Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('Book.Like')]
    TRate = class(TMVCActiveRecord)
    Private
        [MVCTableField('LikeID', [foPrimaryKey, foAutoGenerated])]
        FLikeID: Int64;

        [MVCTableField('BookID')]
        FBookID: Int64;

        [MVCTableField('UserID')]
        FUserID: Int64;

        [MVCTableField('Like')]
        FLike: Byte;

    Public
        Property LikeID: Int64 read FLikeID write FLikeID;
        Property userid: Int64 read FUserID write FUserID;
        Property bookid: Int64 read FBookID write FBookID;
        Property Like: Byte read FLike write FLike;
End;

Implementation

End.
