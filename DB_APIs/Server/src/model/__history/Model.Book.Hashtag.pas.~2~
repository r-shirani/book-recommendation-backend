Unit Model.Book.Hashtag;

Interface

Uses
    MVCFramework.Serializer.Commons,
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables;

Type
  [MVCNameCase(ncLowerCase)]
  [MVCTable('Book.Hashtag')]
  THashtag = class(TMVCActiveRecord)
  private
    [MVCTableField('HashtagCollectionID', [foPrimaryKey, foAutoGenerated])]
    FHashtagCollectionID: Int64;

    [MVCTableField('CollectionID')]
    FCollectionID: NullableInt64;

    [MVCTableField('Title')]
    FTitle: NullableString;

  public
    property HashtagCollectionID: Int64 read FHashtagCollectionID write FHashtagCollectionID;
    property CollectionID: NullableInt64 read FCollectionID write FCollectionID;
    property Title: NullableString read FTitle write FTitle;
  end;

implementation

