Unit Model.User.GenreFavorit;

Interface

Uses
    MVCFramework.Serializer.Commons,
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables;

Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('User.GenreFavorite')]
    TGenreFavorit = class(TMVCActiveRecord)
    Private
        [MVCTableField('GenreFavoriteID', [foPrimaryKey, foAutoGenerated])]
        FGenreFavoriteID: Integer;

        [MVCTableField('UserID')]
        FUserID: Int64;

        [MVCTableField('GenreID')]
        FGenreID: Int64;

        [MVCTableField('Priority')]
        FPriority: NullableInt32;

    Public
        Property GenreFavoriteID: Integer read FGenreFavoriteID write FGenreFavoriteID;
        Property UserID: Int64 read FUserID write FUserID;
        Property GenreID: Int64 read FGenreID write FGenreID;
        Property Priority: NullableInt32 read FPriority write FPriority;
End;

Implementation

End.
